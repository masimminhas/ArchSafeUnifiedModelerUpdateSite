        eType="#//SafetyCriticalBlock" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="systemBlocks" upperBound="-1"
        eType="#//SystemBlock" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="blockAssociations" upperBound="-1"
        eType="#//BlockAssociation" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="blockConnections" upperBound="-1"
