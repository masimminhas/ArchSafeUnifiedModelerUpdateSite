  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="IntegratedHazard" eSuperTypes="#//UnifiedElement">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="System-level dangers that span multiple components with risk level quantification"/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="riskLevel" eType="#//RiskLevel"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="relatedBlocks" upperBound="-1"
        eType="#//SafetyCriticalBlock"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="mitigationStatus" eType="#//MitigationStatus"
        defaultValueLiteral="NOT_MITIGATED"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="hazardCategory" eType="#//HazardCategory"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="severityJustification"
        eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="SafetyCriticalBlock" eSuperTypes="#//SystemBlock">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="System components that require safety analysis with embedded failure modes and criticality levels"/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="safetyCriticality" eType="#//SafetyCriticalityLevel"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="failureModes" upperBound="-1"
        eType="#//BlockFailureMode" containment="true" eOpposite="#//BlockFailureMode/affectedBlock"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="hasRedundancy" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EBoolean"
        defaultValueLiteral="false"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="redundancyType" eType="#//RedundancyType"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="asilLevel" eType="#//ASILLevel"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="SystemBlock" eSuperTypes="#//UnifiedElement">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="Regular system components without mandatory safety annotations for non-critical parts"/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EReference" name="subBlocks" upperBound="-1"
        eType="#//SystemBlock" containment="true" eOpposite="#//SystemBlock/parentBlock"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="parentBlock" eType="#//SystemBlock"
        eOpposite="#//SystemBlock/subBlocks"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="associationAsSource" upperBound="-1"
        eType="#//BlockAssociation" eOpposite="#//BlockAssociation/sourceBlock"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="associationAsTarget" upperBound="-1"
        eType="#//BlockAssociation" eOpposite="#//BlockAssociation/targetBlock"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="connectionAsSource" upperBound="-1"
        eType="#//BlockConnection" eOpposite="#//BlockConnection/fromBlock"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="connectionAsTarget" upperBound="-1"
        eType="#//BlockConnection" eOpposite="#//BlockConnection/toBlock"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="blockType" eType="#//BlockType"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="BlockAssociation" eSuperTypes="#//UnifiedElement">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="associationType" eType="#//AssociationType"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="sourceBlock" eType="#//SystemBlock"
        eOpposite="#//SystemBlock/associationAsSource"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="targetBlock" eType="#//SystemBlock"
        eOpposite="#//SystemBlock/associationAsTarget"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="BlockConnection" eSuperTypes="#//UnifiedElement">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="connectionType" eType="#//ConnectionType"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="fromBlock" lowerBound="1"
        upperBound="-1" eType="#//SystemBlock" eOpposite="#//SystemBlock/connectionAsSource"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="toBlock" lowerBound="1"
        upperBound="-1" eType="#//SystemBlock" eOpposite="#//SystemBlock/connectionAsTarget"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="BlockFailureMode" eSuperTypes="#//UnifiedElement">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="Specific failure scenarios attached directly to system components with detectability and severity metrics"/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EReference" name="affectedBlock" eType="#//SafetyCriticalBlock"
        eOpposite="#//SafetyCriticalBlock/failureModes"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="failureRate" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EDouble"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="failureEffect" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="AnalysisMetadata">
