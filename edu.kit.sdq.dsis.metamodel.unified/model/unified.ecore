<eClassifiers xsi:type="ecore:EClass" name="Requirement" eSuperTypes="#//UnifiedElement" abstract="true">
  <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
    <details key="documentation" value="Base class for all requirements in the safety lifecycle"/>
  </eAnnotations>
  <eStructuralFeatures xsi:type="ecore:EAttribute" name="requirementId" 
      eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
  <eStructuralFeatures xsi:type="ecore:EAttribute" name="requirementText" 
      eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
  <eStructuralFeatures xsi:type="ecore:EAttribute" name="status" 
      eType="#//RequirementStatus" defaultValueLiteral="DRAFT"/>
  <eStructuralFeatures xsi:type="ecore:EAttribute" name="verificationMethod" 
      eType="#//VerificationMethod"/>
  <eStructuralFeatures xsi:type="ecore:EAttribute" name="rationale" 
      eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
  <eStructuralFeatures xsi:type="ecore:EReference" name="derivedFrom" 
      upperBound="-1" eType="#//Requirement"/>
  <eStructuralFeatures xsi:type="ecore:EReference" name="satisfiedBy" 
      upperBound="-1" eType="#//SystemBlock"/>
</eClassifiers>

<eClassifiers xsi:type="ecore:EClass" name="SafetyGoal" eSuperTypes="#//Requirement">
  <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
    <details key="documentation" value="Top-level safety requirement from HARA (ISO 26262-3)"/>
  </eAnnotations>
  <eStructuralFeatures xsi:type="ecore:EAttribute" name="asilLevel" 
      eType="#//ASILLevel"/>
  <eStructuralFeatures xsi:type="ecore:EReference" name="relatedHazard" 
      eType="#//IntegratedHazard"/>
  <eStructuralFeatures xsi:type="ecore:EAttribute" name="safeState" 
      eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
  <eStructuralFeatures xsi:type="ecore:EReference" name="allocatedTo" 
      upperBound="-1" eType="#//FunctionalSafetyRequirement" 
      eOpposite="#//FunctionalSafetyRequirement/allocatedFrom"/>
</eClassifiers>

<eClassifiers xsi:type="ecore:EClass" name="FunctionalSafetyRequirement" 
    eSuperTypes="#//Requirement">
  <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
    <details key="documentation" value="Functional safety requirement (ISO 26262-4)"/>
  </eAnnotations>
  <eStructuralFeatures xsi:type="ecore:EAttribute" name="requirementType" 
      eType="#//FunctionalRequirementType"/>
  <eStructuralFeatures xsi:type="ecore:EReference" name="allocatedFrom" 
      eType="#//SafetyGoal" eOpposite="#//SafetyGoal/allocatedTo"/>
  <eStructuralFeatures xsi:type="ecore:EReference" name="refinedTo" 
      upperBound="-1" eType="#//TechnicalSafetyRequirement" 
      eOpposite="#//TechnicalSafetyRequirement/refinesFrom"/>
  <eStructuralFeatures xsi:type="ecore:EReference" name="implementedBy" 
      upperBound="-1" eType="#//SafetyCriticalBlock"/>
</eClassifiers>

<eClassifiers xsi:type="ecore:EClass" name="TechnicalSafetyRequirement" 
    eSuperTypes="#//Requirement">
  <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
    <details key="documentation" value="Technical safety requirement at system level (ISO 26262-4)"/>
  </eAnnotations>
  <eStructuralFeatures xsi:type="ecore:EAttribute" name="technicalCategory" 
      eType="#//TechnicalRequirementCategory"/>
  <eStructuralFeatures xsi:type="ecore:EAttribute" name="allocatedASIL" 
      eType="#//ASILLevel"/>
  <eStructuralFeatures xsi:type="ecore:EReference" name="refinesFrom" 
      eType="#//FunctionalSafetyRequirement" 
      eOpposite="#//FunctionalSafetyRequirement/refinedTo"/>
  <eStructuralFeatures xsi:type="ecore:EReference" name="realizedBy" 
      upperBound="-1" eType="#//SafetyCriticalBlock"/>
  <eStructuralFeatures xsi:type="ecore:EReference" name="verifiedBy" 
      upperBound="-1" eType="#//FMEAItem"/>
</eClassifiers>

<eClassifiers xsi:type="ecore:EClass" name="SafetyMechanism" 
    eSuperTypes="#//UnifiedElement">
  <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
    <details key="documentation" value="Safety mechanism implementing safety requirements"/>
  </eAnnotations>
  <eStructuralFeatures xsi:type="ecore:EAttribute" name="mechanismType" 
      eType="#//SafetyMechanismType"/>
  <eStructuralFeatures xsi:type="ecore:EAttribute" name="diagnosticCoverage" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
  <eStructuralFeatures xsi:type="ecore:EReference" name="implements" 
      upperBound="-1" eType="#//TechnicalSafetyRequirement"/>
  <eStructuralFeatures xsi:type="ecore:EReference" name="implementedIn" 
      eType="#//SafetyCriticalBlock"/>
	  <!-- Add to SafetyMechanism class: -->
<eStructuralFeatures xsi:type="ecore:EReference" name="validatedBy" upperBound="-1" eType="#//FMEAItem" eOpposite="#//FMEAItem/validatesMechanisms"/>
</eClassifiers>

