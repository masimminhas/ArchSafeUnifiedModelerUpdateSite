        </ownedRules>
        <ownedRules xsi:type="validation:SemanticValidationRule" name="HighRPNRequiresMitigation" level="WARNING" message="High RPN (>100) requires mitigation action." targetClass="unified::FMEAItem">
          <audits auditExpression="aql:(self.severity * self.occurrence * self.detection) &lt;= 100 or (self.recommendedAction &lt;> null and self.recommendedAction.size() > 0)"/>
        </ownedRules>
        <ownedRules xsi:type="validation:SemanticValidationRule" name="CriticalRPNMustBeAddressed" level="ERROR" message="Critical RPN (>200) must have mitigation action and responsible party assigned." targetClass="unified::FMEAItem">
          <audits auditExpression="aql:(self.severity * self.occurrence * self.detection) &lt;= 200 or (self.recommendedAction &lt;> null and self.recommendedAction.size() > 0 and self.actionStatus &lt;> unified::ActionStatus::OPEN)"/>
        </ownedRules>
        <ownedRules xsi:type="validation:SemanticValidationRule" name="AutoGeneratedItemsNeedReview" message="This FMEA item was auto-generated. Please review and refine values." targetClass="unified::FMEAItem">
          <audits auditExpression="aql:not self.autoGenerated"/>
        </ownedRules>
