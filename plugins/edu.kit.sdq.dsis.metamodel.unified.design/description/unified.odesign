          <audits auditExpression="aql:self.severity >= 1 and self.severity &lt;= 10 and self.occurrence >= 1 and self.occurrence &lt;= 10 and self.detection >= 1 and self.detection &lt;= 10"/>
        </ownedRules>
        <ownedRules xsi:type="validation:SemanticValidationRule" name="FMEAItemHasComponent" level="ERROR" message="FMEA item must be linked to a component." targetClass="unified::FMEAItem">
