<?xml version="1.0" encoding="UTF-8"?>
<description:Group xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:description="http://www.eclipse.org/sirius/description/1.1.0" xmlns:description_1="http://www.eclipse.org/sirius/diagram/description/1.1.0" xmlns:description_2="http://www.eclipse.org/sirius/table/description/1.1.0" xmlns:style="http://www.eclipse.org/sirius/diagram/description/style/1.1.0" xmlns:tool="http://www.eclipse.org/sirius/diagram/description/tool/1.1.0" xmlns:tool_1="http://www.eclipse.org/sirius/description/tool/1.1.0" xmlns:validation="http://www.eclipse.org/sirius/description/validation/1.1.0" name="unified" version="12.0.0.2017041100">
  <ownedViewpoints name="UnifiedModeling" modelFileExtension="unified">
    <ownedRepresentations xsi:type="description_1:DiagramDescription" name="ArchitectureDiagram" domainClass="unified::UnifiedSystemModel" enablePopupBars="true">
      <metamodel href="http://edu.kit.sdq.dsis.metamodel/unified#/"/>
      <validationSet name="architectureValidation">
        <ownedRules xsi:type="validation:SemanticValidationRule" name="BlockNameRequired" level="WARNING" message="Block name should be defined for better traceability." targetClass="unified::SystemBlock">
          <audits auditExpression="aql:self.name != null and self.name.trim().size() > 0"/>
        </ownedRules>
        <ownedRules xsi:type="validation:SemanticValidationRule" name="ConnectionTypeDefined" level="WARNING" message="Connection type should be specified." targetClass="unified::BlockConnection">
          <audits auditExpression="aql:self.connectionType != null"/>
        </ownedRules>
        <ownedRules xsi:type="validation:SemanticValidationRule" name="CriticalBlockMustHaveHazards" level="ERROR" message="Safety Critical Block must have associated hazards." targetClass="unified::SafetyCriticalBlock">
          <audits auditExpression="service:hasAssociatedHazards"/>
        </ownedRules>
        <ownedRules xsi:type="validation:SemanticValidationRule" name="HighCriticalityRequiresFMEA" level="ERROR" message="High Criticality block requires FMEA Analysis." targetClass="unified::SafetyCriticalBlock">
          <audits auditExpression="service:requiresFMEAValidation()"/>
        </ownedRules>
        <ownedRules xsi:type="validation:SemanticValidationRule" name="BlockHasConnections" message="Isolated blocks may indicate incomplete architecture." targetClass="unified::SystemBlock">
          <audits auditExpression="aql:self.connectionAsSource->size() > 0 or self.connectionAsTarget->size() > 0"/>
        </ownedRules>
        <ownedRules xsi:type="validation:SemanticValidationRule" name="ASILDRequires80PercentFMEA" level="ERROR" message="ASIL D blocks require 80% FMEA coverage of failure modes." targetClass="unified::SafetyCriticalBlock">
          <audits auditExpression="service:asilDHasFMEACoverage"/>
        </ownedRules>
        <ownedRules xsi:type="validation:SemanticValidationRule" name="NoCircularDependencies" level="ERROR" message="Circular dependency detected. This block is part of a cycle that could cause architectural issues." targetClass="unified::SystemBlock">
          <audits auditExpression="service:checkCircularDependency"/>
        </ownedRules>
        <ownedRules xsi:type="validation:SemanticValidationRule" name="BlocksShouldBeConnected" message="This block has no connections. Isolated blocks may indicate incomplete architecture design." targetClass="unified::SystemBlock">
          <audits auditExpression="aql:self.connectionAsSource->size() > 0 or self.connectionAsTarget->size() > 0"/>
        </ownedRules>
        <ownedRules xsi:type="validation:SemanticValidationRule" name="BlockNameMustBeUnique" level="ERROR" message="Block name must be unique across the model. Duplicate names can cause confusion and traceability issues." targetClass="unified::SystemBlock">
          <audits auditExpression="service:hasUniqueName"/>
        </ownedRules>
        <ownedRules xsi:type="validation:SemanticValidationRule" name="ModelTraceabilityAdequate" level="WARNING" message="Model traceability density is below 30%. Consider adding more links between architecture and safety elements." targetClass="unified::UnifiedSystemModel">
          <audits auditExpression="service:checkTraceabilityCompleteness"/>
        </ownedRules>
        <ownedRules xsi:type="validation:SemanticValidationRule" name="CriticalBlockRequiresRedundancy" level="ERROR" message="CRITICAL safety blocks must have redundancy mechanisms defined." targetClass="unified::SafetyCriticalBlock">
          <audits auditExpression="service:highCriticalityHasRedundancy"/>
        </ownedRules>
      </validationSet>
      <defaultLayer name="ArchitectureLayer">
        <nodeMappings name="AD_SystemBlock_Node" semanticCandidatesExpression="feature:systemBlocks" domainClass="unified::SystemBlock">
          <style xsi:type="style:SquareDescription" labelSize="10" showIcon="false" labelPosition="node" resizeKind="NSEW" width="14" height="6">
            <borderColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            <labelFormat>bold</labelFormat>
            <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            <color xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='light_blue']"/>
          </style>
        </nodeMappings>
        <nodeMappings name="AD_SafetyCriticalBlock_Node" semanticCandidatesExpression="feature:rootBlocks" domainClass="unified::SafetyCriticalBlock">
          <style xsi:type="style:SquareDescription" borderSizeComputationExpression="2" labelSize="10" showIcon="false" labelPosition="node" resizeKind="NSEW" width="14" height="6">
            <borderColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='dark_red']"/>
            <labelFormat>bold</labelFormat>
            <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='dark_red']"/>
            <color xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='light_yellow']"/>
          </style>
        </nodeMappings>
        <edgeMappings name="AD_BlockConnection_Edge" semanticCandidatesExpression="aql:self.blockConnections" sourceMapping="//@ownedViewpoints[name='UnifiedModeling']/@ownedRepresentations[name='ArchitectureDiagram']/@defaultLayer/@nodeMappings[name='AD_SafetyCriticalBlock_Node'] //@ownedViewpoints[name='UnifiedModeling']/@ownedRepresentations[name='ArchitectureDiagram']/@defaultLayer/@nodeMappings[name='AD_SystemBlock_Node']" targetMapping="//@ownedViewpoints[name='UnifiedModeling']/@ownedRepresentations[name='ArchitectureDiagram']/@defaultLayer/@nodeMappings[name='AD_SafetyCriticalBlock_Node'] //@ownedViewpoints[name='UnifiedModeling']/@ownedRepresentations[name='ArchitectureDiagram']/@defaultLayer/@nodeMappings[name='AD_SystemBlock_Node']" targetFinderExpression="aql:self.toBlock" sourceFinderExpression="aql:self.fromBlock" domainClass="unified::BlockConnection" useDomainElement="true">
          <style targetArrow="InputFillClosedArrow" sizeComputationExpression="2">
            <strokeColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            <centerLabelStyleDescription showIcon="false" labelExpression="aql:self.connectionType.toString()">
              <labelFormat>bold</labelFormat>
              <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            </centerLabelStyleDescription>
          </style>
        </edgeMappings>
        <edgeMappings name="AD_BlockAssociation_Edge" semanticCandidatesExpression="aql:self.blockAssociations" sourceMapping="//@ownedViewpoints[name='UnifiedModeling']/@ownedRepresentations[name='ArchitectureDiagram']/@defaultLayer/@nodeMappings[name='AD_SafetyCriticalBlock_Node'] //@ownedViewpoints[name='UnifiedModeling']/@ownedRepresentations[name='ArchitectureDiagram']/@defaultLayer/@nodeMappings[name='AD_SystemBlock_Node']" targetMapping="//@ownedViewpoints[name='UnifiedModeling']/@ownedRepresentations[name='ArchitectureDiagram']/@defaultLayer/@nodeMappings[name='AD_SafetyCriticalBlock_Node'] //@ownedViewpoints[name='UnifiedModeling']/@ownedRepresentations[name='ArchitectureDiagram']/@defaultLayer/@nodeMappings[name='AD_SystemBlock_Node']" targetFinderExpression="aql:self.targetBlock" sourceFinderExpression="aql:self.sourceBlock" domainClass="unified::BlockAssociation" useDomainElement="true">
          <style lineStyle="dash" targetArrow="InputArrowWithDiamond" sizeComputationExpression="2">
            <strokeColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='dark_blue']"/>
            <centerLabelStyleDescription showIcon="false" labelExpression="aql:self.associationType.toString()">
              <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='dark_blue']"/>
            </centerLabelStyleDescription>
          </style>
        </edgeMappings>
        <toolSections name="ArchitectureModeling">
          <ownedTools xsi:type="tool:NodeCreationDescription" name="AD_Create_SystemBlock" label="System Block" nodeMappings="//@ownedViewpoints[name='UnifiedModeling']/@ownedRepresentations[name='ArchitectureDiagram']/@defaultLayer/@nodeMappings[name='AD_SystemBlock_Node']">
            <variable name="container"/>
            <viewVariable name="containerView"/>
            <initialOperation>
              <firstModelOperations xsi:type="tool_1:ChangeContext" browseExpression="var:container">
                <subModelOperations xsi:type="tool_1:CreateInstance" typeName="unified::SystemBlock" referenceName="systemBlocks" variableName="newBlock">
                  <subModelOperations xsi:type="tool_1:SetValue" featureName="name" valueExpression="aql:'SystemBlock_' + (container.systemBlocks->size() + 1)"/>
                  <subModelOperations xsi:type="tool_1:SetValue" featureName="blockType" valueExpression="aql: unified::BlockType::PROCESSING_UNIT"/>
                </subModelOperations>
              </firstModelOperations>
            </initialOperation>
          </ownedTools>
          <ownedTools xsi:type="tool:NodeCreationDescription" name="AD_Create_SafetyCriticalBlock" label="Safety Critical Block" nodeMappings="//@ownedViewpoints[name='UnifiedModeling']/@ownedRepresentations[name='ArchitectureDiagram']/@defaultLayer/@nodeMappings[name='AD_SafetyCriticalBlock_Node']">
            <variable name="container"/>
            <viewVariable name="containerView"/>
            <initialOperation>
              <firstModelOperations xsi:type="tool_1:ChangeContext" browseExpression="var:container">
                <subModelOperations xsi:type="tool_1:CreateInstance" typeName="unified::SafetyCriticalBlock" referenceName="rootBlocks" variableName="newBlock">
                  <subModelOperations xsi:type="tool_1:SetValue" featureName="name" valueExpression="aql:'SafetyBlock_' + (container.rootBlocks->size() + 1)"/>
                  <subModelOperations xsi:type="tool_1:SetValue" featureName="safetyCriticality" valueExpression="aql:unified::SafetyCriticalityLevel::HIGH"/>
                  <subModelOperations xsi:type="tool_1:SetValue" featureName="asilLevel" valueExpression="aql:unified::ASILLevel::ASIL_B"/>
                </subModelOperations>
              </firstModelOperations>
            </initialOperation>
          </ownedTools>
          <ownedTools xsi:type="tool:EdgeCreationDescription" name="AD_Create_BlockConnection" label="Block Connection" edgeMappings="//@ownedViewpoints[name='UnifiedModeling']/@ownedRepresentations[name='ArchitectureDiagram']/@defaultLayer/@edgeMappings[name='AD_BlockConnection_Edge']">
            <sourceVariable name="source"/>
            <targetVariable name="target"/>
            <sourceViewVariable name="sourceView"/>
            <targetViewVariable name="targetView"/>
            <initialOperation>
              <firstModelOperations xsi:type="tool_1:ChangeContext" browseExpression="aql:source.eContainer()">
                <subModelOperations xsi:type="tool_1:CreateInstance" typeName="unified::BlockConnection" referenceName="blockConnections" variableName="newConnection">
                  <subModelOperations xsi:type="tool_1:SetValue" featureName="fromBlock" valueExpression="var:source"/>
                  <subModelOperations xsi:type="tool_1:SetValue" featureName="toBlock" valueExpression="var:target"/>
                  <subModelOperations xsi:type="tool_1:SetValue" featureName="connectionType" valueExpression="aql:unified::ConnectionType::DATA_FLOW"/>
                  <subModelOperations xsi:type="tool_1:SetValue" featureName="name" valueExpression="aql:source.name + '_to_' + target.name"/>
                </subModelOperations>
              </firstModelOperations>
            </initialOperation>
          </ownedTools>
          <ownedTools xsi:type="tool:EdgeCreationDescription" name="AD_Create_BlockAssociation" label="Block Association" edgeMappings="//@ownedViewpoints[name='UnifiedModeling']/@ownedRepresentations[name='ArchitectureDiagram']/@defaultLayer/@edgeMappings[name='AD_BlockAssociation_Edge']">
            <sourceVariable name="source"/>
            <targetVariable name="target"/>
            <sourceViewVariable name="sourceView"/>
            <targetViewVariable name="targetView"/>
            <initialOperation>
              <firstModelOperations xsi:type="tool_1:ChangeContext" browseExpression="aql:source.eContainer()">
                <subModelOperations xsi:type="tool_1:CreateInstance" typeName="unified::BlockAssociation" referenceName="blockAssociations" variableName="newAssociation">
                  <subModelOperations xsi:type="tool_1:SetValue" featureName="sourceBlock" valueExpression="var:source"/>
                  <subModelOperations xsi:type="tool_1:SetValue" featureName="targetBlock" valueExpression="var:target"/>
                  <subModelOperations xsi:type="tool_1:SetValue" featureName="associationType" valueExpression="aql:unified::AssociationType::DEPENDENCY"/>
                  <subModelOperations xsi:type="tool_1:SetValue" featureName="name" valueExpression="aql:source.name + '_assoc_' + target.name"/>
                </subModelOperations>
              </firstModelOperations>
            </initialOperation>
          </ownedTools>
        </toolSections>
        <toolSections name="AnalysisTools" label="Analysis">
          <ownedTools xsi:type="tool_1:OperationAction" name="ValidateConsistency" precondition="aql:true" icon="/edu.kit.sdq.dsis.metamodel.unified.design/icons/validate.gif">
            <view name="views"/>
            <initialOperation>
              <firstModelOperations xsi:type="tool_1:ChangeContext" browseExpression="aql:self">
                <subModelOperations xsi:type="tool_1:ExternalJavaAction" name="Validate" icon="/edu.kit.sdq.dsis.metamodel.unified.design/icons/validate.gif" id="edu.kit.sdq.dsis.unified.design.actions.ValidateConsistencyAction"/>
              </firstModelOperations>
            </initialOperation>
          </ownedTools>
          <ownedTools xsi:type="tool_1:OperationAction" name="ComputeMetrics" label="Compute Model Metrics" icon="/edu.kit.sdq.dsis.metamodel.unified.design/icons/metrics.gif">
            <view name="views"/>
            <initialOperation>
              <firstModelOperations xsi:type="tool_1:ChangeContext" browseExpression="aql:self">
                <subModelOperations xsi:type="tool_1:ExternalJavaAction" name="ComputeMetrics" id="edu.kit.sdq.dsis.unified.design.actions.MetricsAction"/>
              </firstModelOperations>
            </initialOperation>
          </ownedTools>
          <ownedTools xsi:type="tool_1:OperationAction" name="ExportMetricReport" label="Export Metrics Report " precondition="aql:true" icon="/edu.kit.sdq.dsis.metamodel.unified.design/icons/export.gif">
            <view name="views"/>
            <initialOperation>
              <firstModelOperations xsi:type="tool_1:ChangeContext" browseExpression="aql:self">
                <subModelOperations xsi:type="tool_1:ExternalJavaAction" name="ExportMetricsReport" id="edu.kit.sdq.dsis.unified.design.actions.ExportDetailedMetricsAction"/>
              </firstModelOperations>
            </initialOperation>
          </ownedTools>
          <ownedTools xsi:type="tool_1:OperationAction" name="BlockAnalysisMenu" label="Block Analysis" precondition="aql:true" icon="/edu.kit.sdq.dsis.metamodel.unified.design/icons/analyze.gif">
            <view name="views"/>
            <initialOperation>
              <firstModelOperations xsi:type="tool_1:ChangeContext" browseExpression="aql:self">
                <subModelOperations xsi:type="tool_1:ExternalJavaAction" name="BlockAnalysisMenu" label="Block Analysis" id="edu.kit.sdq.dsis.unified.design.actions.ImpactAnalysisAction"/>
              </firstModelOperations>
            </initialOperation>
          </ownedTools>
          <ownedTools xsi:type="tool_1:OperationAction" name="ExportArchitecture" label="Export Architecture" precondition="aql:true" icon="/edu.kit.sdq.dsis.metamodel.unified.design/icons/export-architecture.gif">
            <view name="views"/>
            <initialOperation>
              <firstModelOperations xsi:type="tool_1:ChangeContext" browseExpression="aql:self">
                <subModelOperations xsi:type="tool_1:ExternalJavaAction" name="ExportArchitecture" label="Export Architecture" id="edu.kit.sdq.dsis.unified.design.actions.ExportArchitectureAction"/>
              </firstModelOperations>
            </initialOperation>
          </ownedTools>
          <ownedTools xsi:type="tool:OperationAction" name="ValidateConsistency" label="Validate Consistency" precondition="aql:true">
    <view name="views"/>
    <initialOperation>
      <firstModelOperations xsi:type="tool_1:ExternalJavaAction" name="ValidateConsistency" id="edu.kit.sdq.dsis.unified.design.actions.ValidateConsistencyAction">
                <parameters name="views" value="var:views"/>
              </firstModelOperations>
    </initialOperation>
  </ownedTools>
        </toolSections>
      </defaultLayer>
    </ownedRepresentations>
    <ownedRepresentations xsi:type="description_1:DiagramDescription" name="SafetyDiagram" domainClass="unified::UnifiedSystemModel" enablePopupBars="true">
      <metamodel href="http://edu.kit.sdq.dsis.metamodel/unified#/"/>
      <validationSet name="safetyValidation">
        <ownedRules xsi:type="validation:SemanticValidationRule" name="SafetyBlockNameRequired" level="ERROR" message="Safety critical block name must be defined." targetClass="unified::SafetyCriticalBlock">
          <audits auditExpression="aql:self.name != null and self.name.trim().size() > 0"/>
        </ownedRules>
        <ownedRules xsi:type="validation:SemanticValidationRule" name="HazardNameRequired" level="ERROR" message="Hazard must have a descriptive name." targetClass="unified::IntegratedHazard">
          <audits auditExpression="aql:self.name != null and self.name.trim().size() > 0"/>
        </ownedRules>
        <ownedRules xsi:type="validation:SemanticValidationRule" name="HazardHasRelatedBlocks" level="WARNING" message="Hazard should be linked to at least one safety-critical block." targetClass="unified::IntegratedHazard">
          <audits auditExpression="aql:self.relatedBlocks->size() > 0"/>
        </ownedRules>
        <ownedRules xsi:type="validation:SemanticValidationRule" name="FailureModeHasEffect" level="WARNING" message="Failure mode should describe its effect." targetClass="unified::BlockFailureMode">
          <audits auditExpression="aql:self.failureEffect != null and self.failureEffect.size() > 0"/>
        </ownedRules>
        <ownedRules xsi:type="validation:SemanticValidationRule" name="CriticalHazardMustBeMitigated" level="ERROR" message="Catastrophic hazards must have mitigation strategy." targetClass="unified::IntegratedHazard">
          <audits auditExpression="aql:self.riskLevel &lt;> unified::RiskLevel::CATASTROPHIC or self.mitigationStatus &lt;> unified::MitigationStatus::NOT_MITIGATED"/>
        </ownedRules>
        <ownedRules xsi:type="validation:SemanticValidationRule" name="FailureModeHasProperDescription" level="WARNING" message="Failure mode description should be at least 5 characters for clarity and traceability." targetClass="unified::BlockFailureMode">
          <audits auditExpression="service:hasProperDescription"/>
        </ownedRules>
        <ownedRules xsi:type="validation:SemanticValidationRule" name="ASILLevelDefined" level="WARNING" message="Safety-critical blocks should have an ASIL level defined for automotive compliance." targetClass="unified::SafetyCriticalBlock">
          <audits auditExpression="aql:self.asilLevel> unified::ASILLevel::QM and self.asilLevel != null"/>
        </ownedRules>
        <ownedRules xsi:type="validation:SemanticValidationRule" name="CriticalHazardsMustBeMitigated" level="ERROR" message="Critical and catastrophic hazards must have defined mitigation strategies and be linked to safety mechanisms." targetClass="unified::IntegratedHazard">
          <audits auditExpression="service:criticalHazardHasMitigation"/>
        </ownedRules>
      </validationSet>
      <defaultLayer name="SafetyLayer">
        <nodeMappings name="SD_IntegratedHazard_Node" semanticCandidatesExpression="feature:globalHazards" domainClass="unified::IntegratedHazard">
          <style xsi:type="style:SquareDescription" borderSizeComputationExpression="2" showIcon="false" labelExpression="aql:self.name + ' [' + self.riskLevel.toString() + ']'" iconPath="/edu.kit.sdq.dsis.metamodel.unified.design/icons/IntegratedHazard.png" labelPosition="node" resizeKind="NSEW" width="12" height="5">
            <borderColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            <labelFormat>bold</labelFormat>
            <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='dark_red']"/>
            <color xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='white']"/>
          </style>
        </nodeMappings>
        <edgeMappings name="SD_HazardToBlock_Edge" semanticCandidatesExpression="aql:self.globalHazards" sourceMapping="//@ownedViewpoints[name='UnifiedModeling']/@ownedRepresentations[name='SafetyDiagram']/@defaultLayer/@nodeMappings[name='SD_IntegratedHazard_Node']" targetMapping="//@ownedViewpoints[name='UnifiedModeling']/@ownedRepresentations[name='SafetyDiagram']/@defaultLayer/@containerMappings[name='SD_SafetyCriticalBlock_Container']" targetFinderExpression="aql:self.relatedBlocks" sourceFinderExpression="aql:self" domainClass="unified::IntegratedHazard" useDomainElement="true">
          <style lineStyle="dash_dot" targetArrow="InputClosedArrow" sizeComputationExpression="2">
            <strokeColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='dark_red']"/>
            <centerLabelStyleDescription labelSize="9" showIcon="false" labelExpression="threatens">
              <labelFormat>bold</labelFormat>
              <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='dark_red']"/>
            </centerLabelStyleDescription>
          </style>
        </edgeMappings>
        <containerMappings name="SD_SafetyCriticalBlock_Container" semanticCandidatesExpression="feature:rootBlocks" domainClass="unified::SafetyCriticalBlock" childrenPresentation="List">
          <subNodeMappings name="SD_BlockFailureMode_ListItem" semanticCandidatesExpression="feature:failureModes" domainClass="unified::BlockFailureMode">
            <style xsi:type="style:SquareDescription" labelSize="9" showIcon="false" labelExpression="aql:'  â–º ' + self.name + ' [Î»=' + self.failureRate.toString() + ']'" labelAlignment="LEFT" labelPosition="node" resizeKind="NSEW" height="2">
              <borderColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='light_gray']"/>
              <labelFormat>italic</labelFormat>
              <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='dark_red']"/>
              <color xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='white']"/>
            </style>
          </subNodeMappings>
          <style xsi:type="style:FlatContainerStyleDescription" arcWidth="8" arcHeight="8" borderSizeComputationExpression="2" labelSize="10" showIcon="false" labelExpression="aql:self.name + ' [' + self.asilLevel.toString() + '] [' + self.failureModes->size().toString() + ' FMs]'" roundedCorner="true" widthComputationExpression="20" heightComputationExpression="aql:4 + self.failureModes->size() * 2">
            <borderColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='dark_red']"/>
            <labelFormat>bold</labelFormat>
            <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='dark_red']"/>
            <backgroundColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='white']"/>
            <foregroundColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='light_yellow']"/>
          </style>
        </containerMappings>
        <toolSections name="SafetyTools" label="Safety Analysis Tools">
          <ownedTools xsi:type="tool:ContainerCreationDescription" name="SD_Create_SafetyCriticalBlock" label="Safety Critical Block" containerMappings="//@ownedViewpoints[name='UnifiedModeling']/@ownedRepresentations[name='SafetyDiagram']/@defaultLayer/@containerMappings[name='SD_SafetyCriticalBlock_Container']" icon="/edu.kit.sdq.dsis.metamodel.unified.design/icons/safety-mechanism.gif">
            <variable name="container"/>
            <viewVariable name="containerView"/>
            <initialOperation>
              <firstModelOperations xsi:type="tool_1:ChangeContext" browseExpression="var:container">
                <subModelOperations xsi:type="tool_1:CreateInstance" typeName="unified::SafetyCriticalBlock" referenceName="rootBlocks" variableName="newBlock">
                  <subModelOperations xsi:type="tool_1:SetValue" featureName="name" valueExpression="aql:'SafetyBlock_' + (container.rootBlocks->size() + 1)"/>
                  <subModelOperations xsi:type="tool_1:SetValue" featureName="safetyCriticality" valueExpression="aql:unified::SafetyCriticalityLevel::HIGH"/>
                  <subModelOperations xsi:type="tool_1:SetValue" featureName="asilLevel" valueExpression="aql:unified::ASILLevel::ASIL_B"/>
                  <subModelOperations xsi:type="tool_1:SetValue" featureName="hasRedundancy" valueExpression="false"/>
                </subModelOperations>
              </firstModelOperations>
            </initialOperation>
          </ownedTools>
          <ownedTools xsi:type="tool:NodeCreationDescription" name="SD_Create_IntegratedHazard" label="Integrated Hazard" nodeMappings="//@ownedViewpoints[name='UnifiedModeling']/@ownedRepresentations[name='SafetyDiagram']/@defaultLayer/@nodeMappings[name='SD_IntegratedHazard_Node']" icon="/edu.kit.sdq.dsis.metamodel.unified.design/icons/hazard.gif">
            <variable name="container"/>
            <viewVariable name="containerView"/>
            <initialOperation>
              <firstModelOperations xsi:type="tool_1:ChangeContext" browseExpression="var:container">
                <subModelOperations xsi:type="tool_1:CreateInstance" typeName="unified::IntegratedHazard" referenceName="globalHazards" variableName="newHazard">
                  <subModelOperations xsi:type="tool_1:SetValue" featureName="name" valueExpression="aql:'Hazard_' + (container.globalHazards->size() + 1)"/>
                  <subModelOperations xsi:type="tool_1:SetValue" featureName="riskLevel" valueExpression="aql:unified::RiskLevel::CRITICAL_RISK"/>
                  <subModelOperations xsi:type="tool_1:SetValue" featureName="mitigationStatus" valueExpression="aql:unified::MitigationStatus::NOT_MITIGATED"/>
                  <subModelOperations xsi:type="tool_1:SetValue" featureName="hazardCategory" valueExpression="aql:unified::HazardCategory::FUNCTIONAL_FAILURE"/>
                </subModelOperations>
              </firstModelOperations>
